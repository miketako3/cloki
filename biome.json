{
	"$schema": "https://biomejs.dev/schemas/2.3.10/schema.json",
	"assist": { "actions": { "source": { "organizeImports": "on" } } },
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	},
	"files": {
		"includes": [
			"**",
			"!**/dist/**/*.js",
			"!**/dist/**/*.d.ts",
			"!**/dist/**/*.d.ts.map",
			"!**/dist/**/*.js.map"
		]
	}
}
